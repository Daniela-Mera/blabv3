<h1>Tipo#index</h1>
<p>Find me in app/views/tipo/index.html.erb</p>

<p style="color: green"><%= notice %></p>


<div id="tipos">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Tipo</th>
      </tr>
    </thead>
    <tbody>
      <% @tipos.each do |tipo| %>
        <tr>
          <td><%= tipo.id %></td>
          <td><%= tipo.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>